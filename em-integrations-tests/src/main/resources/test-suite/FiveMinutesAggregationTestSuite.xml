     	 </class>
    </classes>
	</test>
	<test name="SubmeterFiveMinCheck">
	 <classes>
		 <class name="com.ge.current.em.automation.ingestion.AggregationTest">
		   <methods>
        		<include name="testMetadataInFiveMinAggregationForFE2ESubmeter"/>
        		<include name="testMetadataInFiveMinAggregationForFE2ESubmeterSite"/>
        		<include name="testMetadataInFiveMinAggregationForFE2ESubmeterEnterprise"/>
        		<include name="testMetadataInFiveMinAggregationForFE2ESubmeterSegment"/>
