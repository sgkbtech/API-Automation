	 <classes>
		 <class name="com.ge.current.em.automation.ingestion.AggregationTest">
		   <methods>
        		<include name="testMetadataInFiveMinAggregationForFE2EAHU"/>
        		<include name="testMetadataInFiveMinAggregationForFE2EAHUSite"/>
